{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      {
        "source": "/api/old-api",
        "destination": "/api/new-api"
      }
    ],
    "redirects": [
      {
        "source": "/old-page",
        "destination": "/new-page",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Custom-Header",
            "value": "value"
          }
        ]
      }
    ],
    "env": {
      "API_KEY": "JY5VvgG8hY7Ql7pQAgjz9976"
    },
    "regions": ["sfo1"]
}
